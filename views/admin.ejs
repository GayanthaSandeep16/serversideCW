<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Admin Dashboard</title>
  <style>
    table { border-collapse: collapse; width: 100%; }
    th, td { border: 1px solid #ddd; padding: 8px; text-align: left; }
    button { padding: 5px 10px; }
  </style>
</head>
<body>
  <h1>Your API Keys</h1>
  <form action="/admin/generate-key" method="POST">
    <button type="submit">Generate New API Key</button>
  </form>
  <table>
    <tr>
      <th>API Key</th>
      <th>Usage Count</th>
      <th>Last Used</th>
      <th>Status</th>
      <th>Actions</th>
    </tr>
    <% apiKeys.forEach(key => { %>
      <tr>
        <td><%= key.apiKey %></td>
        <td><%= key.ApiKeyUsage ? key.ApiKeyUsage.usageCount : 0 %></td>
        <td><%= key.ApiKeyUsage ? key.ApiKeyUsage.lastUsed : 'Never' %></td>
        <td><%= key.isActive ? 'Active' : 'Revoked' %></td>
        <td>
          <% if (key.isActive) { %>
            <form action="/admin/revoke-key/<%= key.id %>" method="POST">
              <button type="submit">Revoke</button>
            </form>
          <% } else { %>
            Revoked
          <% } %>
        </td>
      </tr>
    <% }) %>
  </table>
</body>
</html>